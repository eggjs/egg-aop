{
  "compilerOptions": {
    "target": "es2017",
    "module": "commonjs",
    "jsx": "react",
    "moduleResolution": "node",
    "noImplicitAny": false,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "preserveConstEnums": true,
    "declaration": true,
    "sourceMap": false,
    "pretty": true,
    "noUnusedLocals": false,
    "noUnusedParameters": true,
    "importHelpers": false
  },
  "compileOnSave": true
}
